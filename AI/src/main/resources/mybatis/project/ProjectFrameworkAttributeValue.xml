<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<!-- 不使用namespace的话sql搜索定位会比较方便 -->
<mapper namespace="ProjectFrameworkAttributeValue">

    <resultMap id="rs_base" type="ProjectFrameworkAttributeValue">
        <result property="id" column="id"/>
        <result property="templateCode" column="templateCode"/>
        <result property="attributeCode" column="attributeCode"/>
        <result property="projectCode" column="projectCode"/>
        <result property="frameworkCode" column="frameworkCode"/>
        <result property="code" column="code"/>
        <result property="attribute" column="attribute"/>
        <result property="attributeValue" column="attributeValue"/>
    </resultMap>

    <!-- 用于select查询公用抽取的列 -->
    <sql id="ProjectFrameworkAttributeValue.columns">
		id,templateCode,attributeCode,projectCode,frameworkCode,code,attribute,attributeValue
	</sql>

    <insert id="ProjectFrameworkAttributeValue.insert" useGeneratedKeys="true" keyProperty="id" parameterType="ProjectFrameworkAttributeValue">
        INSERT INTO `project_framework_attribute_value` (
        id ,
        templateCode ,
        attributeCode ,
        projectCode ,
        frameworkCode ,
        code ,
        attribute ,
        attributeValue
        ) VALUES (
        #{id} ,
        #{templateCode} ,
        #{attributeCode} ,
        #{projectCode} ,
        #{frameworkCode} ,
        #{code} ,
        #{attribute} ,
        #{attributeValue}
        )
        <selectKey keyProperty="id" resultType="long">
            select LAST_INSERT_ID()
        </selectKey>
    </insert>

    <update id="ProjectFrameworkAttributeValue.update" parameterType="ProjectFrameworkAttributeValue">
        UPDATE `project_framework_attribute_value`
        <trim prefix="set" suffixOverrides=",">
            <if test="templateCode!=null and templateCode!=''">
                templateCode = #{templateCode} ,
            </if>
            <if test="attributeCode!=null and attributeCode!=''">
                attributeCode = #{attributeCode} ,
            </if>
            <if test="projectCode!=null and projectCode!=''">
                projectCode = #{projectCode} ,
            </if>
            <if test="frameworkCode!=null and frameworkCode!=''">
                frameworkCode = #{frameworkCode} ,
            </if>
            <if test="code!=null and code!=''">
                code = #{code} ,
            </if>
            <if test="attribute!=null and attribute!=''">
                attribute = #{attribute} ,
            </if>
            <if test="attributeValue!=null and attributeValue!=''">
                attributeValue = #{attributeValue}
            </if>
        </trim>
        WHERE
        id = #{id}
    </update>

    <delete id="ProjectFrameworkAttributeValue.delete">
        DELETE FROM `project_framework_attribute_value`
        <include refid="ProjectFrameworkAttributeValue.where"/>
    </delete>

    <select id="ProjectFrameworkAttributeValue.load" resultMap="rs_base">
        SELECT
        <include refid="ProjectFrameworkAttributeValue.columns"/>
        FROM `project_framework_attribute_value`
        <include refid="ProjectFrameworkAttributeValue.where"/>
    </select>

    <sql id="ProjectFrameworkAttributeValue.where">
        <where>
            <if test="id!=null and id!=''">
                AND id = #{id}
            </if>
            <if test="projectCode!=null and projectCode!=''">
                AND projectCode = #{projectCode}
            </if>
            <if test="templateCode!=null and templateCode!=''">
                AND templateCode = #{templateCode}
            </if>
            <if test="attributeCode!=null and attributeCode!=''">
                AND attributeCode = #{attributeCode}
            </if>
            <if test="frameworkCode!=null and frameworkCode!=''">
                AND frameworkCode = #{frameworkCode}
            </if>
            <if test="code!=null and code!=''">
                AND code = #{code}
            </if>
            <if test="attribute!=null and attribute!=''">
                AND attribute = #{attribute}
            </if>
            <if test="attributeValue!=null and attributeValue!=''">
                AND attributeValue = #{attributeValue}
            </if>
        </where>
    </sql>


    <select id="ProjectFrameworkAttributeValue.count" resultType="long">
        SELECT count(*) FROM `project_framework_attribute_value`
        <include refid="ProjectFrameworkAttributeValue.where"/>
    </select>


    <select id="ProjectFrameworkAttributeValue.query" resultMap="rs_base">
        SELECT
        <include refid="ProjectFrameworkAttributeValue.columns"/>
        FROM `project_framework_attribute_value`
        <include refid="ProjectFrameworkAttributeValue.where"/>

        <if test="sortColumns!=null and sortColumns!=''">
            ORDER BY ${sortColumns}
        </if>
    </select>


</mapper>

